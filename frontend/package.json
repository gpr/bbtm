{
  "name": "bbtm-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:analyze": "ANALYZE=true tsc -b && vite build",
    "build:prod": "tsc -b && vite build --mode production",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "prepare": "husky",
    "analyze": "npm run build:analyze && open dist/stats.html",
    "size": "npm run build:prod && du -sh dist/*"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@mantine/core": "^7.17.8",
    "@mantine/dates": "^7.17.8",
    "@mantine/form": "^7.17.8",
    "@mantine/hooks": "^7.17.8",
    "firebase": "^12.1.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-firebase-hooks": "^5.1.1",
    "react-hook-form": "^7.0.0",
    "react-router-dom": "^6.0.0",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@eslint/js": "^9.35.0",
    "@firebase/rules-unit-testing": "latest",
    "@storybook/addon-a11y": "9.1.7",
    "@storybook/addon-docs": "^9.1.7",
    "@storybook/addon-essentials": "latest",
    "@storybook/addon-interactions": "latest",
    "@storybook/addon-onboarding": "^9.1.7",
    "@storybook/addon-vitest": "9.1.7",
    "@storybook/react-vite": "latest",
    "@storybook/testing-library": "latest",
    "@testing-library/jest-dom": "latest",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "latest",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "latest",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "cypress": "latest",
    "eslint": "^9.35.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^9.1.7",
    "globals": "^16.4.0",
    "husky": "latest",
    "jsdom": "latest",
    "lint-staged": "latest",
    "playwright": "^1.55.0",
    "prettier": "latest",
    "rollup-plugin-visualizer": "^6.0.3",
    "storybook": "latest",
    "terser": "^5.44.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.0.0",
    "vitest": "^3.0.0"
  }
}
